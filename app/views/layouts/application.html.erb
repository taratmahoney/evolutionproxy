  <%= csrf_meta_tags %>
  <link href="../css/froala_style.min.css" rel="stylesheet" type="text/css" />
  <link href='https://fonts.googleapis.com/css?family=Oxygen|Montserrat' rel='stylesheet' type='text/css'>  
